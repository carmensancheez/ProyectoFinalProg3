}

<h2>Eliminar</h2>

<h3>Esta seguro que desea eliminarlo?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Id de Paciente")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Id_Paciente)
        </dd>

        <dt>
            @Html.DisplayName("Id de Habitacion")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Id_Habitacion)
