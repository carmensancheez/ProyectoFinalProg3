@model IEnumerable<ProyectFinalProg3.Models.Altas>

@{
    ViewBag.Title = "Index";
}

<h2>Altas</h2>

<p>
    @Html.ActionLink("Crear Nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Fecha de Inicio")
        </th>
        <th>
            @Html.DisplayName("Monto")
        </th>
        <th>
            @Html.DisplayName("Fecha de Salida")
        </th>
        <th>
            @Html.DisplayName("Nombre del Paciente")
        </th>
        <th>
            @Html.DisplayName("Numero de Habitación")
        </th>
        <th>
            @Html.DisplayName("Fecha de Inicio")
        </th>
        <th>
            @Html.DisplayName("Monto Final")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Ingresos.Fecha_Inicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Salida)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Paciente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IumHabitacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Inicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto_Final)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
